{
  "name": "destiny-insights-frontend",
  "author": "Matt Jarrett",
  "license": "MIT",
  "description": "React SPA for banshee44mods.com",
  "version": "0.24.0",
  "private": true,
  "contributors": [
    {
      "name": "Matt Jarrett",
      "web": "https://mattjarrett.dev"
    }
  ],
  "scripts": {
    "update-cached-mods": "node -e 'require(\"./src/util/build-cached-mods.js\").updateCachedMods()'",
    "commit": "git-cz",
    "lint": "eslint .",
    "fix-lint": "eslint . --fix",
    "test": "react-scripts test --watchAll=false --collectCoverage=true",
    "ci": "npm run lint && npm run test",
    "semantic-release": "semantic-release",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "dependencies": {
    "@material-ui/core": "~4.12.4",
    "@material-ui/icons": "~4.11.3",
    "cross-fetch": "~3.1.5",
    "font-awesome": "~4.7.0",
    "material-ui-search-bar": "~1.0.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-scripts": "5.0.1",
    "recharts": "~2.1.14",
    "web-vitals": "~3.0.2"
  },
  "devDependencies": {
    "@babel/core": "~7.19.3",
    "@babel/eslint-parser": "~7.19.1",
    "@babel/preset-react": "~7.18.6",
    "@commitlint/cli": "~17.1.2",
    "@commitlint/config-conventional": "~17.1.0",
    "@semantic-release/changelog": "~6.0.1",
    "@semantic-release/commit-analyzer": "~9.0.2",
    "@semantic-release/exec": "~6.0.3",
    "@semantic-release/git": "~10.0.1",
    "@semantic-release/github": "~8.0.6",
    "@semantic-release/npm": "~9.0.1",
    "@semantic-release/release-notes-generator": "~10.0.3",
    "@testing-library/jest-dom": "~5.16.5",
    "@testing-library/react": "~13.4.0",
    "@testing-library/user-event": "~14.4.3",
    "commitizen": "~4.2.5",
    "cz-conventional-changelog": "~3.3.0",
    "eslint": "~8.24.0",
    "eslint-plugin-react": "~7.31.8",
    "husky": "~8.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "homepage": ".",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
